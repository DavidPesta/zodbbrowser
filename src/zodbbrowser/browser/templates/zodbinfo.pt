<metal:block metal:use-macro="view/@@standard_macros/page">
<metal:block fill-slot="content">

<tal:block define="object view/obj">
  <div class="object">
    <h1 tal:content="object/id" />
    <h2><i tal:content="object/type"/></h2>
    <h2 tal:content="object/instance_id" />
    <a tal:attributes="href string:${object/path}zodbtree.html">[Zoom in]</a>
    <a href="../zodbtree.html">[Zoom out]</a>
    <h1>History</h1>
    <h2 tal:repeat="history object/history" tal:content="history" />
  </div>
</tal:block>

</metal:block>
</metal:block>
